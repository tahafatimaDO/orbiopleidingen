.btn-yellow{background-color:#ffc107}.course.side-summary{border:1px solid #708090;padding:12px;border-radius:2px}.course.side-summary h3{text-align:center}.course.side-summary .price{font-size:.9rem}.course.side-summary .price strong{font-size:1.3rem;color:#f80}.course.side-summary ul{list-style:none;padding:0}.course.side-summary li{padding-left:1.3em}.course.side-summary li:before{content:"\f00c";font-family:FontAwesome;display:inline-block;margin-left:-1.3em;width:1.3em;color:green}.course .apply{background-color:#f80;font-weight:700;text-align:center;font-size:1.4rem;border-radius:2.5px;padding:4px}.course .apply a{color:#fff;text-decoration:none;font-weight:700}.top-header .logo{display:inline-block}.top-header .logo img{padding:15px;width:10rem}.top-header .quick-links{display:inline-block}.top-header .quick-links ul{list-style:none}.top-header .quick-links ul li{display:inline-block}.top-header .top-contact{text-align:center;display:inline-block;background-color:#ffc107a6;border:1px solid #ffc107;padding:8px}.top-header .top-contact h3{font-weight:700}.course-categories a{text-decoration:none}.course-categories .course-category{background-color:#e0e0e0;border:1px solid #708090;background-position:50%;background-size:cover}.course-categories .course-category h3{margin-top:9rem;text-align:center;background-color:rgba(0,0,0,.39);width:100%;margin-bottom:0;color:#fff}